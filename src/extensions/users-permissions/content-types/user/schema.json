{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "is_validated": {
      "type": "boolean",
      "default": false
    },
    "product_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-review.product-review",
      "mappedBy": "user_id"
    },
    "drafts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::draft.draft",
      "mappedBy": "author_id"
    },
    "newsletters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::newsletter.newsletter",
      "mappedBy": "newsletter_created_by"
    },
    "role_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user-role.user-role",
      "inversedBy": "users"
    },
    "newsletter_drafts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::newsletter-draft.newsletter-draft",
      "mappedBy": "author_id"
    },
    "review_feedbacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review-feedback.review-feedback",
      "mappedBy": "reviewer_id"
    },
    "user_activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-activity.user-activity",
      "mappedBy": "user_id"
    },
    "search_histories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::search-history.search-history",
      "mappedBy": "user_id"
    },
    "digital_footprints": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::digital-footprint.digital-footprint",
      "mappedBy": "user_id"
    },
    "resources": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::resource.resource",
      "mappedBy": "author_id"
    },
    "export_histories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::export-history.export-history",
      "mappedBy": "user_id"
    },
    "subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subscription.subscription",
      "mappedBy": "user_id"
    },
    "product_activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-activity.product-activity",
      "mappedBy": "user_id"
    }
  }
}
