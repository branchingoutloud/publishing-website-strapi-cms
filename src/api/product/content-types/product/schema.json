{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "average_stars": {
      "type": "decimal",
      "default": 0,
      "min": 0,
      "max": 5
    },
    "pros": {
      "type": "text"
    },
    "cons": {
      "type": "text"
    },
    "price_range": {
      "type": "string"
    },
    "vendor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vendor.vendor",
      "inversedBy": "products"
    },
    "product_feature_values": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-feature-value.product-feature-value",
      "mappedBy": "product"
    },
    "product_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-review.product-review",
      "mappedBy": "product_id"
    },
    "review_count": {
      "type": "biginteger"
    },
    "category_reference": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "product_logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "product_activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-activity.product-activity",
      "mappedBy": "product_id"
    }
  }
}
