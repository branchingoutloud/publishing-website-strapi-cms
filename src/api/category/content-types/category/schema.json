{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Product Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "search_terms": {
      "type": "json"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "category_reference"
    },
    "product_features": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comparison-feature.comparison-feature",
      "mappedBy": "product_category_id"
    }
  }
}
